<!DOCTYPE html>
<html>
<head>
  <title></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='main'>

    <%= render 'nav' %>
    <% flash.each do |name, msg| %>
      <div class="flash-message flash-message-<%= name %>">
        <%= msg %>
      </div>
    <% end %>


    <%= yield %>
    
  <div class="clear"></div>
  <%= render 'footer' %>
  </div>

</body>
</html>
